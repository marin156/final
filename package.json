{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^4.1.1",
    "json-server": "0.15.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
